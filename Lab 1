%1

for i=1:9
for j=1:5
M(i,j)=10*i+j;
end
end

M
rozmiar=size(M)
suma=0;

for i=1:rozmiar(1)
for j=1:rozmiar(2)
suma=suma+M(i,j);
end
end

suma


%2

N(1,1)=1;
N(1,2)=2;
N(2,1)=3;
N(2,2)=4;

N

O=N*N

P=N.*N


%3

x=0:0.1:1;
for i=length(x)
    y(i)=0;
    for n=1:10
        y(i)=y(i)+sin(n*x(i))*exp(-n*x(i))/n;
    end
end
plot(x,y)


%4

h=0.1;
x=0:h:1;

y(1)=1;

for i=2:length(x)
    y(i)=y(i-1)+h*x(i-1)^2;
end

y_sc=x.^3/3+1;
plot(x,y,'.',x,y_sc)

%1

for i=1:9
for j=1:5
M(i,j)=10*i+j;
end
end

M
rozmiar=size(M)
suma=0;

for i=1:rozmiar(1)
for j=1:rozmiar(2)
suma=suma+M(i,j);
end
end

suma


%2

Z(1,1)=1;
Z(1,2)=2;
Z(2,1)=3;
Z(2,2)=4;

Z


%3

x=0:0.1:1;
for i=length(x)
    y(i)=0;
    for n=1:10
        y(i)=y(i)+sin(n*x(i))*exp(-n*x(i))/n;
    end
end
plot(x,y)
